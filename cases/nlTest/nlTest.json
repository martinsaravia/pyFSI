
{
    "info": "Analysis of critical velocity of Figures 3.2 and 3.3 of Tosi's Thesis",


    "execution":
    {
        "solver":
        {
            "type":         "pyTransientLeakageFlow",
            "integrator":   "dopri5",
            "processes":     1,
            "rtol": 1E-6,
            "atol": 1E-6
        },
        "time":
        {
            "stepping":         "automatic",
            "startTime":    0,
            "endTime":      1,
            "deltaT":       1E-2
        },
        "debug":"n",
        "output":
        {
            "variables": ["Q0", "U", "V", "A"]
        }
    },


    "mesh":
    {
        "type": "from3Parameters",
        "xi": 0.0,
        "xf": 0.2,
        "dx": 0.0001
    },


    "boundary":[
    {
        "name":     "bTop",
        "type":     "boundary1DRigid",
        "method":   "fromLineByTwoPoints",
        "hi":       5.0E-3,
        "hf":       5.0E-3
    },
    {
        "name":     "bBot",
        "type":     "boundary1DRigid",
        "method":   "fromLineByTwoPoints",
        "hi":       -5.001E-3,
        "hf":       -5.001E-3
    },
    {
        "name":     "beamTop",
        "type":     "boundary1DBeam",
        "beamName": "viga",
        "surface":  "top"
    },
    {
        "name":     "beamBot",
        "type":     "boundary1DBeam",
        "beamName": "viga",
        "surface":  "bot"
    }],


    "solid":
    {
        "name": "viga",
        "formulation": "bernoulliEulerBeam",
        "type": "nnn",
        "section":
        {
            "type": "rect",
            "b": 1,
            "h": 2.0E-4
        },
        "material":
        {
            "name":     "aluminio",
            "type":     "isoElastic",
            "E":        1.1E11,
            "nu":       0.3,
            "rho":      8.78E3
        },
        "solution":
        {
            "type":     "modal",
            "method":   "analytic",
            "modes":    1,
            "damping":  0.01
        },
        "bc":
        {
            "type":     "clampedFree",
            "y0":  0,
            "dy0": 0
        }
    },

    "flow" :
    {
        "name":     "flujo",
        "formulation": "nlLeakageFlow2D",
        "type":     "dualChannel",
        "accel":    "no",
        "units":    "m",
        "fluid":
        {
            "db": "air"
        },
        "thickness": 1,
        "regions":[
        {
            "name":        "topChannel",
            "type":        "top",
            "topBoundary": "bTop",
            "botBoundary": "beamTop"
        },
        {
            "name":        "botChannel",
            "type":        "bot",
            "topBoundary": "beamBot",
            "botBoundary": "bBot"
        }],
        "bc" :
        {
            "type":     "variableInletPressure",
            "inlet":
            {
                "p":    [1E4,  1E4],
                "zeta":  1.0
            },
            "outlet":
            {
                "p": 0,
                "zeta": 0.0
            }
        }
    },

    "fsi":
    {
        "name":         "interaccion",
        "formulation":  "nlLFB1D",
        "type":         "Tosi"
    }
}
